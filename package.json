{
	"name": "registration",
	"version": "0.1.0",
	"bin": {
		"registration": "bin/registration.js"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "TZ=UTC GLOBAL_TABLE='registration' NODE_ENV=test AWS_REGION=us-east-1 EMAIL_SENDER=sender@mailfrom.com jest",
		"cdk": "cdk",
		"lint": "eslint --ext .ts,.js",
		"codegen": "graphql-codegen --config ./src/graphql/codegen.yml",
		"dynamodb:start": "docker run -d --name dynamodb --rm -p 8000:8000 amazon/dynamodb-local",
		"dynamodb:stop": "docker stop dynamodb"
	},
	"devDependencies": {
		"@aws-cdk/aws-appsync": "^1.200.0",
		"@aws-cdk/aws-dynamodb": "^1.200.0",
		"@aws-cdk/aws-lambda": "^1.200.0",
		"@babel/register": "^7.21.0",
		"@faker-js/faker": "^8.0.0",
		"@graphql-codegen/cli": "^3.3.1",
		"@graphql-codegen/typescript": "^3.0.4",
		"@shelf/jest-dynamodb": "^3.4.1",
		"@types/aws-lambda": "^8.10.114",
		"@types/aws-sdk": "^2.7.0",
		"@types/common-tags": "^1.8.1",
		"@types/jest": "^29.4.0",
		"@types/nanoid": "^3.0.0",
		"@types/node": "18.14.6",
		"@types/uuid": "^9.0.1",
		"apollo-boost": "^0.4.9",
		"apollo-server": "^3.12.0",
		"aws-cdk": "2.77.0",
		"aws-cdk-lib": "^2.79.1",
		"aws-lambda": "^1.0.7",
		"aws-sdk": "^2.1374.0",
		"cross-fetch": "^3.1.5",
		"eslint": "^8.40.0",
		"graphql": "^16.6.0",
		"jest": "^29.5.0",
		"mockdate": "^3.0.5",
		"prettier": "^2.8.8",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "~4.9.5"
	},
	"dependencies": {
		"common-tags": "^1.8.2",
		"constructs": "^10.0.0",
		"nanoid": "^4.0.2",
		"source-map-support": "^0.5.21",
		"uuid": "^9.0.0"
	}
